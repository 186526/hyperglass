# Routers
[router.'router1']
address = "192.0.2.1"
asn = "65000"
src_addr_ipv4 = "192.0.2.1"
src_addr_ipv6 = "2001:db8::1"
credential = "default"
location = "pop1"
name = "router1.pop1"
display_name = "Router"
port = "222"
type = "cisco_ios"
proxy = "proxy1"

# Router Credentials
[credential.'default']
username = "username"
password = "password"

[credential.'other']
username = "otheradmin"
password = "otherpass"

# SSH Proxy Servers
[proxy.'proxy1']
address = "10.0.1.1"
username = "username"
password = "password"
type = "linux_ssh"
ssh_command = "ssh -l {username} {host}"
