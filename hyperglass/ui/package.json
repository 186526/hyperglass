    "dayjs": "^1.8.25",
    "framer-motion": "^1.10.0",
    "lodash": "^4.17.15",
    "next": "^9.5.4",
    "react": "^16.13.1",
    "react-countdown": "^2.2.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.7",
    "react-markdown": "^4.3.1",
